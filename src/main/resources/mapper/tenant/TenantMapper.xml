<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.youqusoft.vision.flow.tenant.mapper.TenantMapper">

    <!-- 获取租户信息分页列表 -->
    <select id="getTenantPage" resultType="com.youqusoft.vision.flow.tenant.model.vo.TenantVO">
        SELECT
                id,
                tenant_code,
                name,
                contact_person,
                contact_phone,
                contact_email,
                address,
                industry_type,
                subscription_plan,
                subscription_status,
                expires_at,
                max_users,
                is_active,
                created_at,
                updated_at
        FROM
            saas_tenant
        <where>
        </where>
    </select>

</mapper>
