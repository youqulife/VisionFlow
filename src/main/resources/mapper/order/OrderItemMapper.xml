<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.youqusoft.vision.flow.modules.order.mapper.OrderItemMapper">

    <!-- 获取订单明细分页列表 -->
    <select id="getOrderItemPage" resultType="com.youqusoft.vision.flow.modules.order.model.vo.OrderItemVO">
        SELECT
            id,
            order_id,
            product_id,
            product_name,
            product_code,
            product_type,
            product_price,
            product_quantity,
            product_amount,
            is_deleted,
            create_time,
            update_time
        FROM
            order_item
        <where>
        </where>
    </select>

</mapper>
